<h1>Sign In</h1>

<form action= "<%=session_url%>" method="post">
  <%= form_auth %>
  <label for="username">
    Username
    <input id="username" type="text" name="user[username]">
  </label>
  
  <label for="password">
    Password
    <input id="password" type="text" name="user[password]">
  </label>
  
  <input type="submit" value="submit">
</form>